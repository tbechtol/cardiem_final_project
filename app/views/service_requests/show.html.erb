<div class="page-header">
  <h3>Service Request #<%= @service_request.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Car</dt>
      <dd><%= @service_request.car_id %></dd>

      <dt>Rc phone</dt>
      <dd><%= @service_request.rc_phone %></dd>

      <dt>Driver</dt>
      <dd><%= @service_request.driver_id %></dd>

      <dt>Repair center location</dt>
      <dd><%= @service_request.repair_center_location %></dd>

      <dt>Pickup location</dt>
      <dd><%= @service_request.pickup_location %></dd>

      <dt>Pickup time</dt>
      <dd><%= @service_request.pickup_time %></dd>

      <dt>Pickup date</dt>
      <dd><%= @service_request.pickup_date %></dd>

      <dt>Rc name</dt>
      <dd><%= @service_request.rc_name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/service_requests" class="btn btn-primary">
        Back
      </a>
      <a href="/service_requests/<%= @service_request.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_service_request/<%= @service_request.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
